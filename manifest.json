{
    "manifest_version": 3,
    "name": "MNIT auto Internet login",
    "description" : "allows you to connect automatically to the mnit internet services",
    "version": "1.0",
    "icons": {
        "16": "/img/16.png",
        "48": "/img/48.png",
        "128": "/img/128.png"
    },
    "content_scripts": [
        {
            "matches": ["http://172.16.1.3:8002/*"],
            "js": ["script.js"]
        }
    ]
}